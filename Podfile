# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'

target 'XPUIToolExample' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for XPUIToolExample

  pod 'XPUtil', '2.0.1'
  pod 'SnapKit', '4.0.0'
  
  target 'XPUITool' do
      inherit! :search_paths
      
      pod 'XPUtil', '2.0.1'
      pod 'SnapKit', '4.0.0'
      # Pods for testing
  end

  target 'XPUIToolExampleTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'XPUIToolExampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
